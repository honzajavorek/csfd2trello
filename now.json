{
  "builds": [{ "src": "film2trello/__init__.py", "use": "@now/python" }],
  "routes": [{ "src": "/", "dest": "/film2trello/__init__.py" }],
  "github": {
    "silent": true
  },
  "env": {
    "TRELLO_KEY": "@trello_key",
    "TRELLO_TOKEN": "@trello_token",
    "TRELLO_BOARD": "@trello_board",
    "FLASK_SECRET_KEY": "@flask_secret_key"
  }
}
